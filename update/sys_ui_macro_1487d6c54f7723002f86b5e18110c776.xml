        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	
	<!-- ADD CSS FILE -->
	<g:evaluate>
	  var css = new GlideRecord("content_css");
	  css.addQuery("name", "flow-image-style");
	  css.query()
	  css.next();
	  var cssid = ""+css.sys_id;
	</g:evaluate>
	
	<g2:evaluate>
           var src_name = current.source.participant_name;
		   var dest_name = current.destination.participant_name;
		   var src_protocol;
		if(current.flow_model_name == "PARTNER_SFTP_PUSH_ST_SFTP_PULL_PARTNER"){
			src_protocol = "SFTP";
		} else {
			src_protocol = "PeSIT";
		}
		
		var src_part_type;
		if(current.source.type == "application"){
			src_part_type = "cft";
		} else {
			src_part_type = "part";
		}
	 </g2:evaluate>
	
<link href="${cssid}.cssdbx?" rel="stylesheet" type="text/css"/>
	
	<!-- MAIN CONTENT -->
	<div>
		<div class="" data-type="label" choice="0" type="string" nowrap="true"><label dir="ltr" class="col-xs-12 col-md-1_5 col-lg-2 control-label"><span data-dynamic-title="" mandatory="false" oclass="" class="required-marker"></span><span title="" class="label-text" data-html="false" data-original-title=""></span></label>
		</div>
	
		<div class="col-lg-8">
			<div class="participant $[src_part_type]"><span class="name">$[src_name]</span></div>
			<div class="arrow right">
				<div class="protocol"> $[src_protocol] </div>
				<div class="rectangle push"></div>
				<div class="triangle right"></div>
			</div>
			<div class="participant relay st"><span class="name">ST relay</span></div>
			<div class="arrow left">
				<div class="protocol"> SFTP </div>
				<div class="triangle left"></div>
				<div class="rectangle pull"></div>
				<div class="triangle right"></div>
				<div class="rectangle push-pull"></div>
			</div>
			<div class="participant part"><span class="name">$[dest_name]</span></div>
		</div>
	</div>
</j:jelly>]]></xml>
    </sys_ui_macro>
</record_update>
