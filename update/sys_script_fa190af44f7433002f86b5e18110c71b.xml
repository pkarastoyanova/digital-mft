        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	try { 
	 var r = new sn_ws.RESTMessageV2('x_305439_digital_m.AMFT-Flows', 'Delete a flow');
	 r.setStringParameterNoEscape('businessId', current.cg_id);

	 var response = r.execute();
	 var responseBody = response.getBody();
	 var httpStatus = response.getStatusCode();
		if(httpStatus != 202){
			current.isActionAborted(true);
			gs.addErrorMessage("Failed to remove the flow.");
			gs.error("Failed to remove flow." + responseBody);
		} else {
			gs.info("Flow " + current.flow_name + " has been removed from CG");
			gs.addInfoMessage("Flow " + current.flow_name + " has been removed from CG");
		}
	}
	catch(ex) {
		var message = ex.message;
		gs.error(message);
	}

})(current, previous);]]></script>
